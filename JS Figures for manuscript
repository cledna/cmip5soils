  group_by(day, period) %>%
  summarise(
             N=13,
             mean_tsl= mean(value, na.rm=T),
             sd_tsl=sd(value, na.rm=T),
             se_tsl=sd_tsl/sqrt(N)
  )
Incept

Moll <-  Mollisol_tsl8.5 %>%
  group_by(day, period) %>%
  summarise(
             N=13,
             mean_tsl= mean(value, na.rm=T),
             sd_tsl=sd(value, na.rm=T),
             se_tsl=sd_tsl/sqrt(N)
  )
Moll

Ox <-  Oxisol_tsl8.5 %>%
  group_by(day, period) %>%
  summarise(
             N=13,
             mean_tsl= mean(value, na.rm=T),
             sd_tsl=sd(value, na.rm=T),
             se_tsl=sd_tsl/sqrt(N)
  )
Ox

